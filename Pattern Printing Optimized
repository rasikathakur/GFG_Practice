class Solution {
    public ArrayList<Integer> printPat(int n) {
        ArrayList<Integer> res = new ArrayList<>();
        for(int i = n; i > 0; i--) {
            for(int j = n; j > 0; j--) {
                for(int k = i; k > 0; k--) {
                    res.add(j);
                }
            }
            res.add(-1);
        }    
    
        return res;
    }
}
