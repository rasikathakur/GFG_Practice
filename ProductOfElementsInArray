//Calculate the product of all the elements in an array.
class Solution {

    public static int product(int arr[], int n) {
        int productOfElements = 1;
        for(int i = 0; i < n; i++){
            productOfElements*=arr[i];
        }
        
        return productOfElements;
        
    }
}

//output:
Input:
5
1 2 3 4 5
Output:
120
